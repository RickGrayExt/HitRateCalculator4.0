{
  "ReverseProxy": {
    "Routes": {
      "orchestrator": {
        "ClusterId": "orchestrator",
        "Match": { "Path": "/api/{**catch-all}" },
        "Transforms": [ { "PathPattern": "/{**catch-all}" } ]
      }
    },
    "Clusters": {
      "orchestrator": {
        "Destinations": {
          "d1": { "Address": "http://host.docker.internal:5001" }
        }
      }
    }
  }
}